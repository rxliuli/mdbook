{
  "name": "@liuli-util/mdbook",
  "version": "0.1.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsx ./build.ts && tsc",
    "start": "tsx src/bin.ts"
  },
  "bin": {
    "mdbook": "./bin.mjs"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@liuli-util/async": "^3.4.0",
    "@liuli-util/cli": "^3.21.0",
    "@liuli-util/esbuild-plugins": "^0.1.1",
    "@liuli-util/mdbook-sdk": "workspace:^",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.1",
    "@types/uuid": "^8.3.4",
    "commander": "^9.4.0",
    "esbuild": "^0.15.1",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "jest": "^28.1.3",
    "markdown-util": "workspace:^",
    "ts-jest": "^28.0.7",
    "tsx": "^3.8.1",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "sync": [
    "gitignore",
    "jest"
  ],
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.test.ts"
    ]
  },
  "wallaby": {
    "env": {
      "params": {
        "runner": "--experimental-vm-modules"
      }
    }
  }
}
